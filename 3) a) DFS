#include <iostream>
using namespace std;

int n, a[10][10], v[10], count = 0;

void dfs(int x) {
    count++;
    v[x] = count;

    if (v[x] != 0)
        cout << x << "\t";

    for (int k = 0; k < n; k++)
        if (a[x][k] == 1 && v[k] == 0)
            dfs(k);
}

int main() {
    cout << "\nEnter no. of Nodes: ";
    cin >> n;

    cout << "\nEnter the Adjacency Matrix:-\n";
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            cin >> a[i][j];

    for (int i = 0; i < n; i++)
        v[i] = 0;

    cout << "\nDFS:\t";
    for (int i = 0; i < n; i++)
        if (v[i] == 0)
            dfs(i);

    cout << "\n";

    return 0;
}
